@Test primitive_types
contract Test {
    int x;
    int8 x;
    int16 x;
    int32 x;
    int64 x;
    uint x;
    uint8 x;
    uint16 x;
    uint32 x;
    uint64 x;
    float x;
    double x;
    bool x;
    byte x;
    string x;
    int x, y;
}

@Test primitive_arrays
contract Test {
    int x[10];
    int[10] x;
    int x[10], y[10];
}

@Test missing_semicolon
@Error ERROR_PARSE_FAILED
contract Test {
    int x
}

@Test struct_types
contract Test {
    struct x {
        int a;
        int b;
    }
}
