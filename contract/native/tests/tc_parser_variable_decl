@desc primitive_types
contract test {
    int x;
    int8 x;
    int16 x;
    int32 x;
    int64 x;
    uint x;
    uint8 x;
    uint16 x;
    uint32 x;
    uint64 x;
    float x;
    double x;
    bool x;
    byte x;
    string x;
    int x, y;
}

@desc primitive_arrays
contract test {
    int x[10];
    int x[10], y[10];
}

@desc invalid_array_decl
@return ERROR_PARSE_FAILED
contract test {
    int[10] x;
}

@desc missing_semicolon
@return ERROR_PARSE_FAILED
contract test {
    int x
}

@desc struct_types
contract test {
    struct x {
        int a;
        int b;
    }
    x y;
}
