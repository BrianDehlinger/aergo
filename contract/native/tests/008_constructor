@test(instantiate)
contract callee { }

contract test {
    func f() {
        callee c = new callee();
    }
}

@test(neighbor_reference)
contract neighbor { 
    int v;
    neighbor(int p) {
        v = p;
    }
    func f() int { 
        return v;
    }
}

contract test {
    int v;
    func f() int {
        neighbor n = new neighbor(1);
        v = n.v;
        return n.f();
    }
}

@test(missing_parenthesis)
@error(ERROR_SYNTAX)
contract callee { }

contract test {
    func f() {
        callee c = new callee;
    }
}
