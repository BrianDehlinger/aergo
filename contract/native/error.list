#ifndef error
#define error(code, msg)
#endif

error(WARN_INTERNAL,                    "%s:%d: %s")

error(ERROR_INTERNAL,                   "%s:%d, internal error: %s")
error(ERROR_INVALID_OPTION,             "invalid command option: %s")
error(ERROR_INCOMPATIBLE_VERSION,       "incompatible version with %d.%d")

error(ERROR_DIRECTORY_IO_FAILED,        "%s: %s")
error(ERROR_FILE_IO_FAILED,             "%s: %s")

error(ERROR_PREP_CROSS_IMPORT,          "%s:%d: cross reference with import '%s'")

error(ERROR_SCAN_UNTERMINATED_COMMENT,  "%s:%d:%d: unterminated comment\n%s")
error(ERROR_SCAN_UNTERMINATED_STRING,   "%s:%d:%d: unterminated string\n%s")
error(ERROR_SCAN_UNKNOWN_CHAR,          "%s:%d:%d: unknown character\n%s")
error(ERROR_SCAN_STR_TOO_LONG,          "%s:%d:%d: string literal too long\n%s")
error(ERROR_PARSE_FAILED,               "%s:%d:%d: %s\n%s")

