#ifndef error
#define error(code, msg)
#endif

error(INTERNAL_ERROR,               "%s:%d: %s")
error(INVALID_OPTION_ERROR,         "invalid command line option: %s")

error(DIRECTORY_IO_ERROR,           "%s: %s")
error(FILE_IO_ERROR,                "%s: %s")

error(CROSS_IMPORT_ERROR,           "%s:%d: cross reference with import '%s'")

error(UNTERMINATED_COMMENT_ERROR,   "%s:%d: unterminated comment\n%s")
error(UNTERMINATED_STRING_ERROR,    "%s:%d: unterminated string\n%s")
error(UNKNOWN_CHAR_ERROR,           "%s:%d: unknown character\n%s")
error(SYNTAX_ERROR,                 "%s:%d: %s\n%s")

